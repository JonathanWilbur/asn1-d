#!/usr/bin/make -f
#export DH_VERBOSE = 1

override_dh_auto_test:
        make --makefile=./build/scripts/posix.make test

# Skipped, because the install target uses absolute paths.
override_dh_auto_install:

override_dh_auto_build:
        make --makefile=./build/scripts/posix.make all

override_dh_auto_clean:
        make --makefile=./build/scripts/posix.make clean

%:
        dh $@